{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "Contact",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "lastName": {
      "type": "string",
      "required": true,
      "minLength": 3
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "profession": {
      "type": "enumeration",
      "enum": [
        "developer",
        "designer",
        "marketer"
      ],
      "required": true
    },
    "image": {
      "type": "string",
      "required": true,
      "regex": "^(ftp|http|https):\\/\\/[^ \"]+$"
    },
    "bio": {
      "type": "richtext",
      "required": true,
      "minLength": 10,
      "maxLength": 300
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ],
      "default": "male",
      "required": true
    },
    "dateOfBirth": {
      "type": "date",
      "required": true
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "contacts"
    }
  }
}
